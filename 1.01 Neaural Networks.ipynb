    "# output=ReLU(w1×x1+w2×x2+bias)\n",
    "\n",
    "def neauron_output(x1,x2,w1,w2, bias):\n",
    "\n",
    "        Z = w1 * x1 * w2 * x2 +bias\n",
    "\n",
    "        return max(0,Z)\n",
    "\n",
    "output = neauron_output(3, 5, 0.2, 0.7, -1)\n",
    "\n",
    "print(f'Neauron Output : {output}')"
   ]
  }
